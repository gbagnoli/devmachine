{
  APIAppCredentials:
  {
    ClientID: <%= @conf["ClientID"] %>
    ClientSecret: <%= @conf["ClientSecret"] %>
  }
  Account: <%= @conf["Account"] %>
  SecretsBackendType: auto
  Jobs:
  [
    {
      SourceFolder: /home/giacomo/Pictures
      CreateAlbums: Off
      DeleteAfterUpload: true
      IncludePatterns: ["_IMAGE_EXTENSIONS_", "_ALL_VIDEO_FILES_"]
      ExcludePatterns: []
    }
  ]
}
